<?xml version="1.0" encoding="utf-8" ?>
<Bullets>
	<Bullet>
    <Name>xml_stand_Bullet</Name>
    <Type>Bullet</Type>
    <Components>
			<Component>
        <Type>xml_move</Type>
				<Behavior>
          <Type>xml_move_run</Type>
          <Velocity>
            <X>200</X>
            <Y>0</Y>
          </Velocity>
        </Behavior>
			</Component>
			<Component>
        <Type>xml_render</Type>
				<Behavior>
          <Type>xml_render_run</Type>
          <ResourceName>Bullets/B_Pea</ResourceName>
          <TimeFrame>0.03333</TimeFrame>
          <Bound>
            <X>29</X>
            <Y>22</Y>
          </Bound>
        </Behavior>
			</Component>
			<Component>
        <Type>xml_physic</Type>
			</Component>
			<Component>
        <Type>xml_logic</Type>
        <Behavior>
          <Type>xml_NormalBullet</Type>
        </Behavior>
			</Component>
		</Components>
	</Bullet>

